<p id="notice"><%= notice %></p>

<h1>Dogs</h1>

<table>
  <thead>
    <tr>
      <th>Breed</th>
      <th>Genre</th>
      <th>Castrated</th>
      <th></th>
      <th>Birthday</th>
      <th>Name</th>
      <th>Owner name</th>
      <th>Owner fone</th>
      <th>Last date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dogs.each do |dog| %>
      <tr>
        <td><%= dog.breed %></td>
        <td><%= dog.genre %></td>
        <td><%= dog.castrated %></td>
        <td><%= dog. %></td>
        <td><%= dog.birthday %></td>
        <td><%= dog.name %></td>
        <td><%= dog.owner_name %></td>
        <td><%= dog.owner_fone %></td>
        <td><%= dog.last_date %></td>
        <td><%= link_to 'Show', dog %></td>
        <td><%= link_to 'Edit', edit_dog_path(dog) %></td>
        <td><%= link_to 'Destroy', dog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dog', new_dog_path %>
